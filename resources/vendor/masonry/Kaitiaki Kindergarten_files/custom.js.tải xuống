;(function ($) {

  'use strict'

  $('.intro-text .owl-carousel').owlCarousel({
      loop: true,
      margin: 0,
      nav: true,
      items: 1,
  })

  $('.owl-carousel').owlCarousel({
    loop: true,
    margin: 0,
    nav: false,
    dots: true,
    items: 1,
  })

  $('.masonry-columns').masonry({
    itemSelector: '.item-masonry',
    percentPosition: true
  });


})(jQuery);